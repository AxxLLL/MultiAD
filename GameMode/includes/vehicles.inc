findVehicleModelByNamePart(namepart[]) {
	new length;
	if(!isnull(namepart) && (length = strlen(namepart)) < 15) {
		for(new i; i < sizeof(g_VehicleList); i ++) {
			if(strcmp(namepart, g_VehicleList[i][VEHICLE_NAME], true, length) == 0) return g_VehicleList[i][VEHICLE_MODEL];
		}
	}
	return 0;
}
//------------------------------------------------------------------------------
getVehicleNameByModel(modelid, name[], size = sizeof(name)) {
	if(611 >= modelid >= 400) {
		strunpack(name, g_VehicleList[(modelid - 400)][VEHICLE_NAME], size);
		return 1;
	}	
	return 0;
}
//------------------------------------------------------------------------------
stock getVehicleFlagsByModel(modelid) return ((611 >= modelid >= 400) ? g_VehicleList[modelid - 400][VEHICLE_FLAG] : VEHICLE_INVALID_TO_SPAWN);
//==============================================================================
//------------------------------------------------------------------------------
//==============================================================================
loadVehicleData() {
	if(fexist(DIR_GENERAL)) {	    
	    new read[128];
	    if(!fexist(FILE_VEHICLES)) {
			saveVehicleData();
			getConsoleMessage(read, CONSOLE_MESSAGE_NEW_CONFIG_FILE);
            printf(read, "Vehicle Data", FILE_VEHICLES);
	    } else {
			new File:f_open;
			new index, length;
			new modelid = -1, flag;
			
			f_open = fopen(FILE_VEHICLES, io_read);
			
			while(fread(f_open, read, sizeof(read))) {
			    DeleteNewLine(read);
			    DeleteSpaces(read);
				if(!isnull(read) && (((length = (strlen(read) - 1)) > (index = strfind(read, ":")) > 0) || (length > (index = strfind(read, "=")) > 0))) {
					if(611 >= (modelid = strval(read)) >= 400 && !(getVehicleFlagsByModel(modelid) & VEHICLE_INVALID_TO_SPAWN) && 6 >= (flag = strval(read[index + 1])) >= 0) {
						g_VehicleList[(modelid - 400)][VEHICLE_FLAG] = flag;
					}
				}
			}
			fclose(f_open);
		}
		
		return 1;
	}
	return 0;
}
//------------------------------------------------------------------------------
saveVehicleData() {
	if(fexist(DIR_GENERAL)) {
		new File:f_open = fopen(FILE_VEHICLES, io_write);
		new write[16];
		for(new i; i < sizeof(g_VehicleList); i ++) {
			if(!(getVehicleFlagsByIndex(i) & VEHICLE_INVALID_TO_SPAWN)) {
				format(write, sizeof(write), "\r\n%d:%d", getVehicleModelByIndex(i), getVehicleFlagsByIndex(i));
				fwrite(f_open, write);
			}
		}		
		fclose(f_open);
		return 1;
	}
	return 0;
}

